<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.www.customercenter.mapper.CustomerCenterMapper">

	<select id="getQnA" resultType="com.project.www.customercenter.dto.QnA">
		select * from q_n_board
	</select>


	<select id="getInquiryBoard" resultType="com.project.www.customercenter.dto.InquiryBoard">
		select
		Inquiry_id,id,title,date,email
		from inquiry_board order by Inquiry_id desc
	</select>

</mapper>
